[unit]
Description=SCANS_monitoring
Requires=display-manager.service
After=syslog.target network-online.target
Wants=syslog.target network-online.target
[Service]
ExecStart=/Users/James/Code/NMR/SCANS/Client/bin/scans_start
ExecStop=/Users/James/Code/NMR/SCANS/Client/bin/scans_stop
Restart=on-failure
RestartSec=2

[Install]
WantedBy=multi-user.target